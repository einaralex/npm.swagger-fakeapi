---
parser: babel-eslint
extends: icelandair

env:
  browser: false
  node: true
  es6: true

rules:
  brace-style: [error, stroustrup]
  global-require: off
  no-console: off
  no-param-reassign: off
  no-underscore-dangle: off
  no-unsafe-finally: off
  no-useless-computed-key: off
  no-restricted-syntax: off
